"*":
  core:
    audioBeep: false
    customFileTypes:
      "source.js": [
        "ts"
      ]
    disabledPackages: [
      "atom-jshint"
      "parcel"
      "grunt-runner"
      "exception-reporting"
    ]
    ignoredNames: [
      ".sass-cache"
      ".DS_Store"
      "node_modules"
      ".css.map"
      ".js.map"
    ]
    openEmptyEditorOnStart: false
    telemetryConsent: "no"
    themes: [
      "native-ui"
      "one-dark-syntax"
    ]
  editor:
    fontFamily: "Source Code Pro"
    invisibles: {}
    showInvisibles: true
  "fuzzy-finder":
    ignoredNames: [
      "vendor"
      "node_modules"
    ]
    preserveLastSearch: true
    useAlternateScoring: true
  "grunt-runner":
    gruntPaths: [
      "/usr/local/bin"
    ]
  linter:
    errorPanelHeight: 24
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "open-recent": {}
  "svg-preview":
    openPreviewAutomatically: true
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    showOnRightSide: true
    sortFoldersBeforeFiles: false
    squashDirectoryNames: true
  "unity-ui":
    showIcons: true
  welcome:
    showOnStartup: false
